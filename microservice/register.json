{
  "name": "aqueduct-analysis-microservice",
  "cache": [
    "aqueduct"
  ],
  "endpoints": [
    {
      "path": "/v1/aqueduct/analysis",
      "method": "GET",
      "binary": true,
      "redirect": {
        "method": "GET",
        "path": "/api/v1/aqueduct/analysis"
      }
    },
    {
      "path": "/v1/aqueduct/analysis",
      "method": "POST",
      "binary": true,
      "redirect": {
        "method": "POST",
        "path": "/api/v1/aqueduct/analysis"
      }
    },
    {
      "path": "/v1/aqueduct/analysis/cba",
      "method": "GET",
      "binary": true,
      "redirect": {
        "method": "GET",
        "path": "/api/v1/aqueduct/analysis/cba"
      }
    },
    {
      "path": "/v1/aqueduct/analysis/cba/widget/:widget_id",
      "method": "GET",
      "binary": true,
      "redirect": {
        "method": "GET",
        "path": "/api/v1/aqueduct/analysis/cba/widget/:widget_id"
      }
    },
    {
      "path": "/v1/aqueduct/analysis/cba/default",
      "method": "GET",
      "binary": true,
      "redirect": {
        "method": "GET",
        "path": "/api/v1/aqueduct/analysis/cba/default"
      }
    },
    {
      "path": "/v1/aqueduct/analysis/risk/widget/:widget_id",
      "method": "GET",
      "binary": true,
      "redirect": {
        "method": "GET",
        "path": "/api/v1/aqueduct/analysis/risk/widget/:widget_id"
      }
    },
    {
      "path": "/v1/aqueduct/analysis/cba/expire-cache",
      "method": "DELETE",
      "authenticated": true,
      "redirect": {
        "method": "POST",
        "path": "/api/v1/aqueduct/analysis/cba/expire-cache"
      }
    }
  ]
}
